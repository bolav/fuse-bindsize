<App Theme="Basic">
	<JavaScript>
		var Observable = require('FuseJS/Observable');
		var sizex = Observable(0);
		var sizey = Observable(0);

		function c () {
			debug_log("x: " + sizex);
			debug_log("y: " + sizey);
		}

		module.exports = {
			sizex: sizex,
			sizey: sizey,
			c: c
		}	
	</JavaScript>
	<BindPanel SizeX="{sizex}" SizeY="{sizey}">
		<Button Text="Click me" Clicked="{c}" />
	</BindPanel>
</App>

