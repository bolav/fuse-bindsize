<App Theme="Basic">
	<JavaScript>
		var Observable = require('FuseJS/Observable');
		var size = Observable([0,0]);

		function c () {
			debug_log("s: " + size.value);
		} 

		module.exports = {
			size: size,
			c: c
		}	
	</JavaScript>
	<BindPanel Size="{size}">
		<Button Text="Click me" Clicked="{c}" />
	</BindPanel>
</App>

